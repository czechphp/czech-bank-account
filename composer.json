{
    "name": "czechphp/czech-bank-account",
    "type": "library",
    "description": "Provides useful utilities for czech bank account validation and data manipulation.",
    "keywords": [
        "bank account number",
        "bank code",
        "payment system code",
        "variable symbol",
        "specific symbol",
        "kód banky",
        "číselník kódů platebního styku",
        "Czech republic",
        "Czechia"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/cache-contracts": "^2 || ^3",
        "symfony/http-client-contracts": "^2 || ^3",
        "symfony/deprecation-contracts": "^2 || ^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "Lukáš Mencl",
            "email": "tetra.jinak@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Czechphp\\CzechBankAccount\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czechphp\\CzechBankAccount\\Tests\\": "tests/"
        }
    }
}
